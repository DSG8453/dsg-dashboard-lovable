{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "build",
  "installCommand": "echo 'Skip - dependencies installed in build command'",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://dsg-backend-564085662748.us-central1.run.app/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
